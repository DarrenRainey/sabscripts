<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--TV Shows Root directory-->
    <!--Multiple Directories Separted by a Semi-Colon-->
    <add key="tvRoot" value="D:\TV;" />
    <!--<add key="tvRoot" value="D:\TV;Z:\Documents and Settings\John\My Documents\My Videos\TV" />-->
    <!--TV Show naming format. you can copy this directly from SAB's sorting tab-->
    <add key="tvTemplate" value="%sn\%sx%0e - %en.%ext" />
    <!--Same as tvTemplate but for daily episodes that use date instead of seasons and episode number-->
    <add key="tvDailyTemplate" value="%t/%t - %y-%0m-%0d - %.desc.%ext" />
    <!--Extension of the files that should be considered as video files-->
    <add key="videoExt" value=".mkv;.avi;.wmv" />
    <!--Ignore list. use this to ignore seasons. that season and any seasons prior to that number are ignored. -->
    <add key="ignoreSeasons" value="" />
    <!--Directory where SAB archives it's NZB files.-->
    <add key="nzbDir" value="C:\Users\John\AppData\Local\sabnzbd\nzbs" />
    <!--IP address and port of your SAB server-->
    <add key="sabnzbdInfo" value="localhost:8080" />
    <!--SAB's username-->
    <add key="username" value="" />
    <!--SAB's password-->
    <add key="password" value="" />
    <!--SAB's API Key-->
    <add key="apiKey" value="f69587b268240706dec238e57d8843ca" />
    <!--Queue priority of the shows added using this program. -1/0/1 (Low/Normal/High)-->
    <add key="priority" value="1" />
    <!--Whether or not you have set SAB to replace illegal characters.-->
    <add key="sabReplaceChars" value="true" />
    <!--Download Quality for TVnzb-->
    <!--Download Quality use: xvid or 720p (all in lower-case), leaving both will download the first one it finds, used for TVNZB and other sites, not used for NZBs.org, NZBMatrix or Newzbin-->
    <add key="downloadQuality" value="xvid" />
    <!--Should PROPERs be download? Will Delete Non-proper from disk-->
    <add key="downloadPropers" value="true" />
    <!--RSS list file name-->
    <add key="rss" value="RSS.config" />
    <!--Alias list file name-->
    <add key="alias" value="Alias.config" />
    <!--Alias list file name-->
    <add key="quality" value="Quality.config" />
    <!--Verbose Logging-->
    <add key="verboseLogging" value="false" />
    <!--Verbose Logging-->
    <add key="deleteLogs" value="60" />
    <!-- Delete logs for SABSync after X days, setting to 0 will keep logs forever.-->
  </appSettings>
</configuration>